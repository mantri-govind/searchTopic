

<%= form_for @content, html: { multipart: true }  do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :tags %>
  <%= f.text_field :tags %>
  <%= f.label :attachment %>
  <%= f.file_field :attachment %>
  <%= f.submit "Save", class: "btn btn-primary" %>
  

<% end %>


